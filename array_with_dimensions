# 17. Create an array with specified dimensions and names for each dimension

# Vector of values
values <- 1:12

# Vector of dimensions
dims <- c(3, 2, 2)

# Names for each dimension
dimnames <- list(c("Dim1_1", "Dim1_2", "Dim1_3"), c("Dim2_1", "Dim2_2"), c("Dim3_1", "Dim3_2"))

# Create array
array_data <- array(values, dim = dims, dimnames = dimnames)

cat("Array:\n")
print(array_data)
