# 8. Program to extract specified letters from the alphabet

first_10_lowercase <- letters[1:10]
last_10_uppercase <- LETTERS[17:26]
letters_22_to_24_uppercase <- toupper(letters[22:24])

cat("First 10 lowercase letters:", first_10_lowercase, "\n")
cat("Last 10 uppercase letters:", last_10_uppercase, "\n")
cat("Letters 22 to 24 in uppercase:", letters_22_to_24_uppercase, "\n")
